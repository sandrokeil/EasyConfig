{
    "name": "sandrokeil/easy-config",
    "description": "Zend Framework 2 (zf2) module to retrieve specific module options and provides some abstract factories to create easily instances depending on configuration",
    "homepage": "https://github.com/sandrokeil/EasyConfig",
    "license": "BSD-3-Clause",
    "type": "library",
    "keywords": [
        "zf2",
        "zend framework",
        "service manager",
        "factory",
        "config",
        "service",
        "options"
    ],
    "authors": [
        {"name": "Sandro Keil", "homepage": "https://github.com/sandrokeil"}
    ],
    "autoload": {
        "psr-4": {
            "Sake\\EasyConfig\\": "src/",
            "SakeTest\\EasyConfig\\": "test/"
        }
    },
    "require": {
        "php": "~5.3",
        "zendframework/zend-servicemanager": "~2.2"
    },
    "require-dev": {
        "zendframework/zend-test": "~2.2",
        "zendframework/zend-config": "~2.2",
        "zendframework/zend-form": "~2.2",
        "zendframework/zend-serializer": "~2.2",
        "zendframework/zend-log": "~2.2",
        "zendframework/zend-modulemanager": "~2.2",
        "sandrokeil/citools": "~1.0"
    },
    "suggest": {
        "zendframework/zend-stdlib": "Zend\\StdLib for AbstractOptionHydratorConfigFactory"
    }
}

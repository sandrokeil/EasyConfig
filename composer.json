{
    "name": "sandrokeil/easy-config",
    "description": "Easy config provides some abstract factories to easily create instances depending on configuration or retrieve specified module options.",
    "homepage": "https://github.com/sandrokeil/EasyConfig",
    "license": "BSD-3-Clause",
    "type": "library",
    "keywords": [
        "zf2",
        "config",
        "service",
        "options"
    ],
    "authors": [
        {"name": "Sandro Keil"}
    ],
    "support": {
        "issues": "https://github.com/sandrokeil/EasyConfig/issues",
        "source": "https://github.com/sandrokeil/EasyConfig"
    },
    "autoload": {
        "psr-0": {
            "Sake\\EasyConfig": "src/",
            "SakeTest\\EasyConfig": "tests/"
        }
    },
    "require": {
        "php": ">=5.3.3",
        "zendframework/zend-servicemanager": "~2.2"
    },
    "require-dev": {
        "php": ">=5.3.3",
        "zendframework/zendframework": "~2.2",
        "phpunit/phpunit": ">=3.7",
        "phpunit/php-invoker": "*",
        "phpunit/phpunit-story": "*",
        "phpmd/phpmd": ">=1.4",
        "squizlabs/php_codesniffer": ">=1.4",
        "satooshi/php-coveralls": "~0.6"
    },
    "suggest": {
        "zendframework/zend-stdlib": "Zend\\StdLib for AbstractOptionHydratorConfigFactory"
    }
}

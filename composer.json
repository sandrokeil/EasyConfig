{
    "name": "sandrokeil/easy-config",
    "description": "Easy config provides some abstract factories to easily create instances depending on configuration or retrieve specified module options.",
    "homepage": "https://github.com/sandrokeil/EasyConfig",
    "license": "BSD-3-Clause",
    "type": "library",
    "keywords": [
        "zf2",
        "config",
        "service",
        "options"
    ],
    "autoload": {
        "psr-0": {
            "Sake\\EasyConfig": "src/",
            "SakeTest\\EasyConfig": "tests/"
        }
    },
    "require": {
        "php": ">=5.3.3",
        "zendframework/zend-servicemanager": "~2.1"
    },
    "require-dev": {
        "php": ">=5.3.3",
        "zendframework/zend-servicemanager": "~2.1",
        "phpunit/phpunit":           ">=3.7",
        "phpunit/php-invoker": "1.1.*",
        "phpmd/phpmd":               "1.4.*",
        "squizlabs/php_codesniffer": "1.4.*",
        "satooshi/php-coveralls":    "~0.6"
    }
}
